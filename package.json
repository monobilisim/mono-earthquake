{
  "name": "frontend",
  "version": "0.0.1",
  "devDependencies": {
    "@internationalized/date": "^3.11.0",
    "@lucide/svelte": "^0.574.0",
    "@sveltejs/kit": "^2.52.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@types/bun": "^1.3.9",
    "bits-ui": "2.11.0",
    "chart.js": "^4.5.1",
    "clsx": "^2.1.1",
    "iconv-lite": "^0.7.2",
    "mode-watcher": "^1.1.0",
    "prettier": "^3.8.1",
    "prettier-plugin-svelte": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "svelte": "^5.51.3",
    "svelte-adapter-bun": "^1.0.1",
    "svelte-check": "^4.4.0",
    "svelte-sonner": "^1.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5.9.3",
    "vite": "npm:rolldown-vite@^7.3.1"
  },
  "private": true,
  "scripts": {
    "dev": "bun --bun run vite dev",
    "build": "BUILD_STEP=1 bun --bun run vite build",
    "preview": "bun --bun run vite preview",
    "prepare": "bun --bun run svelte-kit sync || echo ''",
    "check": "bun --bun run svelte-kit sync && bun --bun run svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "bun --bun run svelte-kit sync && bun --bun run svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "bun --bun run prettier --write . --ignore-path .prettierignore",
    "lint": "bun --bun run prettier --check .",
    "apply:schema": "bun --bun run src/lib/schema.ts"
  },
  "type": "module"
}
